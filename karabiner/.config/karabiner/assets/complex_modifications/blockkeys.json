{
  "title": "Block Keys",
  "author": "Julian Iaquinandi",
  "homepage": "",
  "hostpage": "",
  "manual": "",
  "import_url": "karabiner://karabiner/assets/complex_modifications/import?url=https://raw.githubusercontent.com/Vonng/Capslock/master/mac/capslock.json",
  "rules": [
    {
      "description": "Block Keys",
      "manipulators": [
        {
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": ["right_option"]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter",
              "modifiers": ["right_option"]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
